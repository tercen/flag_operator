{
  "name": "Flag operator",
  "description": "Flag text or numerical values based on a condition.",
  "tags": ["data wrangling", "text processing"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/flag_operator"],
  "properties": [
    {
    "kind": "EnumeratedProperty",
    "name": "type",
    "defaultValue": "numeric",
    "values": [
      "numeric",
      "character"
    ],
    "description": "Type of data to flag."
  },
    {
    "kind": "EnumeratedProperty",
    "name": "comparison",
    "defaultValue": "equals",
    "values": [
      "equals",
      "greater",
      "less",
      "greater_or_equals",
      "less_or_equals",
      "top",
      "bottom",
      "is_in",
      "contains"
    ],
    "description": "Comparison to perform"
  },
    {
    "kind": "StringProperty",
    "name": "value",
    "defaultValue": "",
    "description": "Value to compare data to."
  }
  ]
}


